{
  "name": "underscore",
  "version": "0.0.1",
  "description": "underscore + 完整测试",
  "main": "underscore.js",
  "scripts": {
    "test": "standard && tman tests/*.js",
    "test-cov": "node node_modules/.bin/istanbul cover tman -- -t 20000",
    "lint": "standard",
    "fix": "standard --fix",
    "build": "rollup -c --environment DEPS -f umd -o dist/underscore.js",
    "build:umd": "rollup -c -f umd -o dist/underscore.umd.js",
    "dev": "rollup -c -w -f umd -o dist/underscore.umd.js",
    "build:browser": "rollup -c --environment DEPS -f umd -o dist/underscore.deps.js",
    "build:es": "rollup -c -f es6 -o dist/underscore.es.js",
    "prepublish": "npm test && npm run build:es"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aladdin-ADD/underscore.git"
  },
  "keywords": [
    "underscore",
    "functional programming"
  ],
  "author": "Aladdin-ADD",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Aladdin-ADD/underscore/issues"
  },
  "bin": {
    "istanbul": "./node_modules/.bin/istanbul",
    "eslint": "./node_modules/.bin/eslint"
  },
  "homepage": "https://github.com/Aladdin-ADD/underscore#readme",
  "devDependencies": {
    "buble": "latest",
    "eslint": "latest",
    "istanbul": "latest",
    "rollup": "^0.36.3",
    "rollup-plugin-buble": "^0.8.0",
    "rollup-plugin-commonjs": "^2.2.1",
    "rollup-plugin-json": "^2.0.0",
    "rollup-plugin-node-resolve": "^1.5.0",
    "rollup-watch": "^2.5.0",
    "source-map": "^0.5.6",
    "source-map-support": "^0.4.0",
    "standard": "^8.6.0",
    "tman": "latest"
  },
  "files": [
    "README.md",
    "bin",
    "underscore.js",
    "tests"
  ],
  "standard": {
    "ignore": [
      "node_modules",
      "dist",
      "_underscore.js"
    ]
  }
}
