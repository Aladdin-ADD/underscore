{
  "name": "underscore",
  "version": "0.0.1",
  "description": "underscore + 完整测试",
  "main": "underscore.js",
  "scripts": {
    "test": "standard && node ./node_modules/.bin/istanbul cover tman --colors ./tests/*.js",
    "test-cov": "istanbul cover _tman 'tests/*.js'",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "lint": "standard",
    "fix": "standard --fix",
    "build": "rollup -c --environment DEPS -f umd -o dist/underscore.js",
    "build:umd": "rollup -c -f umd -o dist/underscore.umd.js",
    "dev": "rollup -c -w -f umd -o dist/underscore.umd.js",
    "build:browser": "rollup -c --environment DEPS -f umd -o dist/underscore.deps.js",
    "build:es": "rollup -c -f es6 -o dist/underscore.es.js",
    "prepublish": "npm test && npm run build:es"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aladdin-ADD/underscore.git"
  },
  "keywords": [
    "underscore",
    "functional programming"
  ],
  "author": "Aladdin-ADD",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Aladdin-ADD/underscore/issues"
  },
  "bin": {
    "istanbul": "./node_modules/.bin/istanbul",
    "eslint": "./node_modules/.bin/eslint"
  },
  "homepage": "https://github.com/Aladdin-ADD/underscore#readme",
  "devDependencies": {
    "buble": "latest",
    "istanbul": "latest",
    "rollup": "latest",
    "rollup-plugin-buble": "latest",
    "rollup-plugin-commonjs": "latest",
    "rollup-plugin-json": "latest",
    "rollup-plugin-node-resolve": "latest",
    "rollup-watch": "latest",
    "source-map": "latest",
    "source-map-support": "latest",
    "standard": "latest",
    "tman": "latest"
  },
  "files": [
    "README.md",
    "bin",
    "underscore.js",
    "tests"
  ],
  "standard": {
    "ignore": [
      "node_modules",
      "dist",
      "_underscore.js"
    ]
  }
}
